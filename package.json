    "eslint-plugin-import": "^2.22.1",
    "parcel": "^2.0.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "path-browserify": "^1.0.1",
    "prettier": "2.7.1",
    "process": "^0.11.10",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
